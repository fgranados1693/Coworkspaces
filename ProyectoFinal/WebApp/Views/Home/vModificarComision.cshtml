@using WebApp.Helpers;
<script src="~/Scripts/Views/vModificarComision.js"></script>
@{ 
    Layout = "~/Views/Shared/_LayoutForms.cshtml"; 
}


<div class="container">
    <div class="row">
        <div class="col align-self-center">
            <div>
                <h5>Modificar comisión</h5>
            </div>
            <form id="formTelefono">

                @Html.CtrlInput(id: "txtComision", type: "number", label: "Comisión", columnDataName: "Comision", placeHolder: "")

            </form>
            <div class="alert alert-success alert-dismissable" style="display: none" id="alert_container">
                <button type="button" class="close" onclick="$('.alert').hide()">&times;</button>
                <span id="alert_message"></span>
            </div>
            <div>
                @Html.CtrlButton(viewName: "vModificarComision", id: "btnMod", label: "Guardar cambios", onClickFunction: "Mod", buttonType: "primary")
            </div>
        </div>
    </div>
</div>
